#!/usr/bin/bash

QPROF=$HOME/go/bin/qprof

$QPROF -db hpcc_monitoring_db -host http://localhost:8086 -out ./profiles "SELECT MAX(*) FROM CPU_Temperature WHERE host='10.101.1.1' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-20T23:59:59Z' GROUP BY *, time(1h) SLIMIT 1; SELECT MIN(*) FROM CPU_Temperature WHERE host='10.101.1.1' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-20T23:59:59Z' GROUP BY *, time(1h) SLIMIT 1; SELECT MEAN(*) FROM CPU_Temperature WHERE host='10.101.1.1' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-20T23:59:59Z' GROUP BY *, time(1h) SLIMIT 1; SELECT MAX(*) FROM Inlet_Temperature WHERE host='10.101.1.1' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-20T23:59:59Z' GROUP BY *, time(1h) SLIMIT 1; SELECT MIN(*) FROM Inlet_Temperature WHERE host='10.101.1.1' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-20T23:59:59Z' GROUP BY *, time(1h) SLIMIT 1; SELECT MEAN(*) FROM Inlet_Temperature WHERE host='10.101.1.1' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-20T23:59:59Z' GROUP BY *, time(1h) SLIMIT 1; SELECT MAX(*) FROM CPU_Usage WHERE host='10.101.1.1' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-20T23:59:59Z' GROUP BY *, time(1h) SLIMIT 1; SELECT MIN(*) FROM CPU_Usage WHERE host='10.101.1.1' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-20T23:59:59Z' GROUP BY *, time(1h) SLIMIT 1; SELECT MEAN(*) FROM CPU_Usage WHERE host='10.101.1.1' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-20T23:59:59Z' GROUP BY *, time(1h) SLIMIT 1;"