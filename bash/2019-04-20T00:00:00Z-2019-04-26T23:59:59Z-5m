#!/usr/bin/bash

QPROF=$HOME/go/bin/qprof

$QPROF -db hpcc_monitoring_db -host http://localhost:8086 -out ./profiles -n 8 "SELECT MAX(*) FROM Fan_Speed WHERE host='10.101.10.39' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-26T23:59:59Z' GROUP BY *, time(5m) SLIMIT 1; SELECT MAX(*) FROM Inlet_Temperature WHERE host='10.101.1.28' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-26T23:59:59Z' GROUP BY *, time(5m) SLIMIT 1; SELECT MIN(*) FROM Memory_Usage WHERE host='10.101.5.10' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-26T23:59:59Z' GROUP BY *, time(5m) SLIMIT 1; SELECT MIN(*) FROM Inlet_Temperature WHERE host='10.101.5.16' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-26T23:59:59Z' GROUP BY *, time(5m) SLIMIT 1; SELECT MEAN(*) FROM Inlet_Temperature WHERE host='10.101.5.5' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-26T23:59:59Z' GROUP BY *, time(5m) SLIMIT 1; SELECT MAX(*) FROM CPU_Usage WHERE host='10.101.10.39' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-26T23:59:59Z' GROUP BY *, time(5m) SLIMIT 1; SELECT MAX(*) FROM Node_Power_Usage WHERE host='10.101.7.51' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-26T23:59:59Z' GROUP BY *, time(5m) SLIMIT 1; SELECT MAX(*) FROM CPU_Usage WHERE host='10.101.2.6' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-26T23:59:59Z' GROUP BY *, time(5m) SLIMIT 1; SELECT MIN(*) FROM Node_Power_Usage WHERE host='10.101.10.43' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-26T23:59:59Z' GROUP BY *, time(5m) SLIMIT 1; SELECT MAX(*) FROM CPU_Temperature WHERE host='10.101.8.19' AND time >= '2019-04-20T00:00:00Z' AND time <= '2019-04-26T23:59:59Z' GROUP BY *, time(5m) SLIMIT 1;"